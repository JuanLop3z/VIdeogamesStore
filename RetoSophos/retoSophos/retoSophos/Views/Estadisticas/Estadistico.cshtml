@model IEnumerable<retoSophos.Models.Clientes>

@{
    ViewBag.Title = "Estadistico";
}

<h2>Estadistico</h2>

    @model List<retoSophos.Models.Clientes>

    <table>
        <thead>
            <tr>
                <th>Nombre del campo 1</th>
                <th>Nombre del campo 2</th>
                <th>Nombre del campo 3</th>
                <th>Nombre del campo 3</th>
                <th>Nombre del campo 3</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Identificacion</td>
                    <td>@item.Nombres</td>
                    <td>@item.Estado</td>
                    <td>@item.Frecuencia</td>
                    <td>@item.Edad</td>
                </tr>
            }
        </tbody>
    </table>